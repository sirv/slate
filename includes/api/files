  <span class="dl">&quot;</span><span class="s2">path</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">/v2/files/jwt</span><span class="dl">&quot;</span><span class="p">,</span>
