<p><h2 id='scroll-through-search-results'>Scroll through search results</h2><div class="highlight"><pre class="highlight python tab-python"><code><span class="kn">import</span> <span class="nn">http.client</span></p>

<p><span class="n">conn</span> <span class="o">=</span> <span class="n">http</span><span class="p">.</span><span class="n">client</span><span class="p">.</span><span class="n">HTTPSConnection</span><span class="p">(</span><span class="s">&quot;api.sirv.com&quot;</span><span class="p">)</span></p>

<p><span class="n">payload</span> <span class="o">=</span> <span class="s">&quot;{}&quot;</span></p>

<p><span class="n">headers</span> <span class="o">=</span> <span class="p">{</span>
    <span class="s">&#39;content-type&#39;</span><span class="p">:</span> <span class="s">&quot;application/json&quot;</span><span class="p">,</span>
    <span class="s">&#39;authorization&#39;</span><span class="p">:</span> <span class="s">&quot;Bearer BEARER_TOKEN_HERE&quot;</span>
    <span class="p">}</span></p>

<p><span class="n">conn</span><span class="p">.</span><span class="n">request</span><span class="p">(</span><span class="s">&quot;POST&quot;</span><span class="p">,</span> <span class="s">&quot;/v2/files/search/scroll&quot;</span><span class="p">,</span> <span class="n">payload</span><span class="p">,</span> <span class="n">headers</span><span class="p">)</span></p>

<p><span class="n">res</span> <span class="o">=</span> <span class="n">conn</span><span class="p">.</span><span class="n">getresponse</span><span class="p">()</span>
<span class="n">data</span> <span class="o">=</span> <span class="n">res</span><span class="p">.</span><span class="n">read</span><span class="p">()</span></p>

<p><span class="k">print</span><span class="p">(</span><span class="n">data</span><span class="p">.</span><span class="n">decode</span><span class="p">(</span><span class="s">&quot;utf-8&quot;</span><span class="p">))</span>
</code></pre></div><div class="highlight"><pre class="highlight shell tab-shell"><code>curl <span class="nt">--request</span> POST <span class="se">\</span>
  <span class="nt">--url</span> https://api.sirv.com/v2/files/search/scroll <span class="se">\</span>
  <span class="nt">--header</span> <span class="s1">&#39;authorization: Bearer BEARER_TOKEN_HERE&#39;</span> <span class="se">\</span>
  <span class="nt">--header</span> <span class="s1">&#39;content-type: application/json&#39;</span> <span class="se">\</span>
  <span class="nt">--data</span> <span class="s1">&#39;{}&#39;</span>
</code></pre></div><div class="highlight"><pre class="highlight javascript tab-javascript--node"><code><span class="kd">var</span> <span class="nx">http</span> <span class="o">=</span> <span class="nx">require</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">https</span><span class="dl">&quot;</span><span class="p">);</span></p>

<p><span class="kd">var</span> <span class="nx">options</span> <span class="o">=</span> <span class="p">{</span>
  <span class="dl">&quot;</span><span class="s2">method</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">POST</span><span class="dl">&quot;</span><span class="p">,</span>
  <span class="dl">&quot;</span><span class="s2">hostname</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">api.sirv.com</span><span class="dl">&quot;</span><span class="p">,</span>
  <span class="dl">&quot;</span><span class="s2">port</span><span class="dl">&quot;</span><span class="p">:</span> <span class="kc">null</span><span class="p">,</span>
  <span class="dl">&quot;</span><span class="s2">path</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">/v2/files/search/scroll</span><span class="dl">&quot;</span><span class="p">,</span>
  <span class="dl">&quot;</span><span class="s2">headers</span><span class="dl">&quot;</span><span class="p">:</span> <span class="p">{</span>
    <span class="dl">&quot;</span><span class="s2">content-type</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">application/json</span><span class="dl">&quot;</span><span class="p">,</span>
    <span class="dl">&quot;</span><span class="s2">authorization</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">Bearer BEARER_TOKEN_HERE</span><span class="dl">&quot;</span>
  <span class="p">}</span>
<span class="p">};</span></p>

<p><span class="kd">var</span> <span class="nx">req</span> <span class="o">=</span> <span class="nx">http</span><span class="p">.</span><span class="nx">request</span><span class="p">(</span><span class="nx">options</span><span class="p">,</span> <span class="kd">function</span> <span class="p">(</span><span class="nx">res</span><span class="p">)</span> <span class="p">{</span>
  <span class="kd">var</span> <span class="nx">chunks</span> <span class="o">=</span> <span class="p">[];</span></p>

<p><span class="nx">res</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">data</span><span class="dl">&quot;</span><span class="p">,</span> <span class="kd">function</span> <span class="p">(</span><span class="nx">chunk</span><span class="p">)</span> <span class="p">{</span>
    <span class="nx">chunks</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span><span class="nx">chunk</span><span class="p">);</span>
  <span class="p">});</span></p>

