  <span class="dl">&quot;</span><span class="s2">path</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">/v2/files/upload?filename=%2FREST%20API%20Examples%2Fuploaded.txt</span><span class="dl">&quot;</span><span class="p">,</span>
  <span class="dl">&quot;</span><span class="s2">headers</span><span class="dl">&quot;</span><span class="p">:</span> <span class="p">{</span>
