<p><span class="k">func</span> <span class="n">main</span><span class="p">()</span> <span class="p">{</span></p>

<p><span class="n">url</span> <span class="o">:=</span> <span class="s">&quot;https://api.sirv.com/v2/files/upload?filename=%2FREST%20API%20Examples%2Fuploaded.txt&quot;</span></p>

<p><span class="n">req</span><span class="p">,</span> <span class="n">_</span> <span class="o">:=</span> <span class="n">http</span><span class="o">.</span><span class="n">NewRequest</span><span class="p">(</span><span class="s">&quot;POST&quot;</span><span class="p">,</span> <span class="n">url</span><span class="p">,</span> <span class="no">nil</span><span class="p">)</span></p>

