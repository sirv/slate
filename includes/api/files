<p><span class="nx">xhr</span><span class="p">.</span><span class="nx">open</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">GET</span><span class="dl">&quot;</span><span class="p">,</span> <span class="dl">&quot;</span><span class="s2">https://api.sirv.com/v2/files/options?filename=%2FREST%20API%20Examples</span><span class="dl">&quot;</span><span class="p">);</span>
