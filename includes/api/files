    <span class="dl">&quot;</span><span class="s2">authorization</span><span class="dl">&quot;</span><span class="p">:</span> <span class="dl">&quot;</span><span class="s2">Bearer BEARER_TOKEN_HERE</span><span class="dl">&quot;</span>
  <span class="p">}</span>
<span class="p">};</span></p>

<p><span class="kd">var</span> <span class="nx">req</span> <span class="o">=</span> <span class="nx">http</span><span class="p">.</span><span class="nx">request</span><span class="p">(</span><span class="nx">options</span><span class="p">,</span> <span class="kd">function</span> <span class="p">(</span><span class="nx">res</span><span class="p">)</span> <span class="p">{</span>
  <span class="kd">var</span> <span class="nx">chunks</span> <span class="o">=</span> <span class="p">[];</span></p>

<p><span class="nx">res</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">data</span><span class="dl">&quot;</span><span class="p">,</span> <span class="kd">function</span> <span class="p">(</span><span class="nx">chunk</span><span class="p">)</span> <span class="p">{</span>
    <span class="nx">chunks</span><span class="p">.</span><span class="nx">push</span><span class="p">(</span><span class="nx">chunk</span><span class="p">);</span>
  <span class="p">});</span></p>

<p><span class="nx">res</span><span class="p">.</span><span class="nx">on</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">end</span><span class="dl">&quot;</span><span class="p">,</span> <span class="kd">function</span> <span class="p">()</span> <span class="p">{</span>
    <span class="kd">var</span> <span class="nx">body</span> <span class="o">=</span> <span class="nx">Buffer</span><span class="p">.</span><span class="nx">concat</span><span class="p">(</span><span class="nx">chunks</span><span class="p">);</span>
    <span class="nx">console</span><span class="p">.</span><span class="nx">log</span><span class="p">(</span><span class="nx">body</span><span class="p">.</span><span class="nx">toString</span><span class="p">());</span>
  <span class="p">});</span>
<span class="p">});</span></p>

<p><span class="nx">req</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">{</span><span class="se">\&quot;</span><span class="s2">filename</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">/REST API Examples/aurora.jpg</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">key</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">cIxWUYfCjRHAy1BpcoIVNI5TivXszVPq</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">alias</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">demo-jwt</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">secureParams</span><span class="se">\&quot;</span><span class="s2">:{</span><span class="se">\&quot;</span><span class="s2">w</span><span class="se">\&quot;</span><span class="s2">:300,</span><span class="se">\&quot;</span><span class="s2">h</span><span class="se">\&quot;</span><span class="s2">:300},</span><span class="se">\&quot;</span><span class="s2">expiresIn</span><span class="se">\&quot;</span><span class="s2">:300}</span><span class="dl">&quot;</span><span class="p">);</span>
<span class="nx">req</span><span class="p">.</span><span class="nx">end</span><span class="p">();</span>
</code></pre></div><div class="highlight"><pre class="highlight javascript tab-javascript"><code><span class="kd">var</span> <span class="nx">data</span> <span class="o">=</span> <span class="dl">&quot;</span><span class="s2">{</span><span class="se">\&quot;</span><span class="s2">filename</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">/REST API Examples/aurora.jpg</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">key</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">cIxWUYfCjRHAy1BpcoIVNI5TivXszVPq</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">alias</span><span class="se">\&quot;</span><span class="s2">:</span><span class="se">\&quot;</span><span class="s2">demo-jwt</span><span class="se">\&quot;</span><span class="s2">,</span><span class="se">\&quot;</span><span class="s2">secureParams</span><span class="se">\&quot;</span><span class="s2">:{</span><span class="se">\&quot;</span><span class="s2">w</span><span class="se">\&quot;</span><span class="s2">:300,</span><span class="se">\&quot;</span><span class="s2">h</span><span class="se">\&quot;</span><span class="s2">:300},</span><span class="se">\&quot;</span><span class="s2">expiresIn</span><span class="se">\&quot;</span><span class="s2">:300}</span><span class="dl">&quot;</span><span class="p">;</span></p>

<p><span class="kd">var</span> <span class="nx">xhr</span> <span class="o">=</span> <span class="k">new</span> <span class="nx">XMLHttpRequest</span><span class="p">();</span>
<span class="nx">xhr</span><span class="p">.</span><span class="nx">withCredentials</span> <span class="o">=</span> <span class="kc">true</span><span class="p">;</span></p>

