<p><span class="nx">xhr</span><span class="p">.</span><span class="nx">open</span><span class="p">(</span><span class="dl">&quot;</span><span class="s2">POST</span><span class="dl">&quot;</span><span class="p">,</span> <span class="dl">&quot;</span><span class="s2">https://api.sirv.com/v2/account/events/search</span><span class="dl">&quot;</span><span class="p">);</span>
